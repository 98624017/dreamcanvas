{
  "productName": "DreamCanvas",
  "version": "0.1.0",
  "identifier": "com.dreamcanvas.app",
  "build": {
    "frontendDist": "./launcher"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "DreamCanvas",
        "width": 1280,
        "height": 800,
        "minWidth": 960,
        "minHeight": 640,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico",
      "icons/icon.icns",
      "icons/icon.png"
    ]
  }
}
